  <input type="hidden" name="EDITSCREEN" value="on" />
  <script>
    function SubmitForm(){document.getElementById("JASFORM").submit();};
    function Add(){
      document.getElementById('EDIT_ACTION').value='ADD';
      document.getElementById('EDIT_JBLOK_JBLOK_UID').value=document.getElementById('EDIT_JBLOK_JBLOK_UID').value;
      SubmitForm();
    };
    function Cancel(){
      document.getElementById('EDIT_ACTION').value='';
      document.getElementById('EDIT_JBLOK_JBLOK_UID').value='';
      SubmitForm();
    };
  </script>


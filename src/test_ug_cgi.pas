// Testing Jegas' CGI Unit - rename to nph-cgitest.cgi after compiling.
program test_ug_cgi;
//=============================================================================

//=============================================================================
// Global Directives
//=============================================================================
{$INCLUDE i_jegas_splash.pp}
{$mode objfpc}
//=============================================================================

//=============================================================================
Uses
//=============================================================================
  ug_cgiin,
  ug_cgiout;
//=============================================================================


//=============================================================================
Var
//=============================================================================
  MYCGI: JFC_CGIENV;
//=============================================================================

//=============================================================================
Begin
//=============================================================================
  If i8CGIIN=0 Then
  Begin
    MYCGI:=JFC_CGIENV.Create;
    MYCGI.CGIINIT(rCGIIN);
    Writeln(MYCGI.saNPHCGITEST);
    MYCGI.Destroy;    
  End;
End.

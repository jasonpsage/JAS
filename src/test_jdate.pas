Var
//=============================================================================
  dt: TDATETIME;
  ts: TTIMESTAMP;
  sa: AnsiString;
//=============================================================================

//=============================================================================
Begin
//=============================================================================
    sa:='1905-01-01 00:00:00';
    Writeln(JDate(sa,1,11));
    Writeln(JDate(sa,12,11));
    
End.
